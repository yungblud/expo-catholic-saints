{
  "version": "0.3.0",
  "name": "catholic-saints-search",
  "description": "카톨릭 성인 검색 앱",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts,.tsx,.json",
    "lint:fix": "eslint . --ext .ts,.tsx,.json --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "typecheck": "tsc --noEmit",
    "deploy:staging": "npx expo export -p web && npx sst deploy --stage staging",
    "deploy:production": "npx expo export -p web && npx sst deploy --stage production",
    "sst:dev": "npx sst dev",
    "sst:remove:staging": "npx sst remove --stage staging",
    "sst:remove:production": "npx sst remove --stage production",
    "prepare": "lefthook install || true",
    "generate:sitemap": "node scripts/generate-sitemap.mjs",
    "fix:static-html": "node scripts/fix-static-html.mjs"
  },
  "keywords": [
    "expo",
    "react-native",
    "catholic",
    "saints"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@expo/vector-icons": "^15.1.1",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-navigation/bottom-tabs": "^7.0.0",
    "expo": "~54.0.33",
    "expo-build-properties": "~1.0.10",
    "expo-constants": "~18.0.13",
    "expo-dev-client": "~6.0.20",
    "expo-font": "~14.0.11",
    "expo-haptics": "~15.0.8",
    "expo-linking": "~8.0.11",
    "expo-router": "~6.0.23",
    "expo-splash-screen": "~31.0.13",
    "expo-sqlite": "~16.0.10",
    "expo-status-bar": "~3.0.9",
    "expo-system-ui": "~6.0.9",
    "expo-updates": "~29.0.16",
    "expo-web-browser": "~15.0.10",
    "fuse.js": "^7.0.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "~4.1.6",
    "react-native-safe-area-context": "5.6.2",
    "react-native-screens": "~4.16.0",
    "react-native-web": "~0.21.2",
    "react-native-worklets": "0.5.1",
    "tinybase": "^5.0.0",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.0",
    "@commitlint/cli": "^20.3.1",
    "@commitlint/config-conventional": "^20.3.1",
    "@expo/metro-runtime": "~6.1.2",
    "@testing-library/jest-native": "^5.4.0",
    "@testing-library/react-native": "^12.4.0",
    "@types/jest": "^29.5.0",
    "@types/react": "~19.1.17",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.0",
    "eslint-config-expo": "~10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "jest": "^29.7.0",
    "jest-expo": "~54.0.17",
    "lefthook": "^2.0.16",
    "nativewind": "^4.0.0",
    "prettier": "^3.2.0",
    "sst": "^3.18.1",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.3.0"
  },
  "private": true,
  "featureFlags": {},
  "pnpm": {
    "overrides": {
      "@expo/metro-runtime": "6.1.2"
    }
  }
}
