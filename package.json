{
  "version": "0.1.0",
  "name": "catholic-saints-search",
  "description": "카톨릭 성인 검색 앱",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.json --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "typecheck": "tsc --noEmit",
    "deploy:staging": "npx expo export -p web && npx sst deploy --stage staging",
    "deploy:production": "npx expo export -p web && npx sst deploy --stage production",
    "sst:dev": "npx sst dev",
    "sst:remove:staging": "npx sst remove --stage staging",
    "sst:remove:production": "npx sst remove --stage production",
    "prepare": "lefthook install || true",
    "generate:sitemap": "node scripts/generate-sitemap.mjs",
    "fix:static-html": "node scripts/fix-static-html.mjs"
  },
  "keywords": [
    "expo",
    "react-native",
    "catholic",
    "saints"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@expo/vector-icons": "^14.0.0",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-navigation/bottom-tabs": "^7.0.0",
    "expo": "~52.0.0",
    "expo-build-properties": "~0.13.3",
    "expo-constants": "~17.0.0",
    "expo-dev-client": "~5.0.20",
    "expo-font": "~13.0.0",
    "expo-haptics": "~14.0.1",
    "expo-linking": "~7.0.0",
    "expo-router": "~4.0.0",
    "expo-splash-screen": "~0.29.0",
    "expo-sqlite": "~15.1.4",
    "expo-status-bar": "~2.0.0",
    "expo-system-ui": "~4.0.0",
    "expo-updates": "~0.27.4",
    "expo-web-browser": "~14.0.0",
    "fuse.js": "^7.0.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.6",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-web": "~0.19.13",
    "tinybase": "^5.0.0",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.0",
    "@commitlint/cli": "^20.3.1",
    "@commitlint/config-conventional": "^20.3.1",
    "@testing-library/jest-native": "^5.4.0",
    "@testing-library/react-native": "^12.4.0",
    "@types/jest": "^29.5.0",
    "@types/react": "~18.3.0",
    "@types/react-native": "~0.73.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.0",
    "eslint-config-expo": "~8.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "jest": "^29.7.0",
    "jest-expo": "~52.0.0",
    "lefthook": "^2.0.16",
    "nativewind": "^4.0.0",
    "prettier": "^3.2.0",
    "sst": "^3.18.1",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.3.0"
  },
  "private": true,
  "featureFlags": {}
}
